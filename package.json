  "scripts": {
    "start-dev": "SET NODE_ENV=development&&SET NODE_CONFIG=dev&&webpack-dev-server --config webpack/webpack.config.js",
    "start-stage": "SET NODE_ENV=development&&SET NODE_CONFIG=stage&&webpack-dev-server --config webpack/webpack.config.js",
    "start-dev-bash": "NODE_ENV=development NODE_CONFIG=dev webpack-dev-server --config webpack/webpack.config.js",
    "start-stage-bash": "NODE_ENV=development NODE_CONFIG=stage webpack-dev-server --config webpack/webpack.config.js",
