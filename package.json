    "start-48": "serve ./dist -s -p 81",
    "prod": "serve ./dist -s",
    "lint": "eslint . --ext .js,.jsx",
    "ngrok": "ngrok http -region eu 8080",
    "test": "jest",
    "precommit": "lint-staged"
